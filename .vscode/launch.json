{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Main + Renderer",
      "configurations": ["Main", "Renderer"],
      "stopAll": true
    }
  ],
  "configurations": [
    {
      "name": "Main",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "args": "start",
      "outputCapture": "std",
      "console": "integratedTerminal"
    },
    {
      "name": "Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9222
    }
  ]
}
